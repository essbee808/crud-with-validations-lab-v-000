<%= form_for @song do |f| %>
  <label>Title</label><br>
    <%= f.text_field :title %>

  <label>Released</label><br>
    <%= f.check_box :released %>
    <br>
  <label>Release Year</label><br>
    <%= f.number_field :release_year %>

  <label>Artist Name</label><br>
    <%= f.text_field :artist_name %>

  <label>Genre</label><br>
    <%= f.text_field :genre %>
    
  <%= f.submit %>
<% end %>
